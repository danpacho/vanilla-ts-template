{
    "name": "vanilla-ts-template",
    "private": true,
    "version": "0.0.0",
    "author": "danpacho",
    "type": "module",
    "scripts": {
        "vite:dev": "vite",
        "vite:build": "tsc && vite build",
        "vite:preview": "vite preview",
