        "eslint-plugin-prettier": "^4.2.1",
        "gitmoji-cli": "^7.0.2",
        "husky": "^8.0.2",
        "lint-staged": "^13.0.3",
        "prettier": "^2.7.1",
        "typescript": "^4.8.4",
        "vite": "^3.2.3"
    }
